@model IEnumerable<WAFF.DataAccess.ViewModels.Demographics.voterCount>

@{
    int count = 0;
    int total = 0;

    foreach (var education in Model)
    {
        <input type="hidden" id="@("graphEducation" + count++)" value="@education.VoterCount" />
        total = total + education.VoterCount;
    }

    <input type="hidden" id="graphEducationTotal" value="@total" />
}
