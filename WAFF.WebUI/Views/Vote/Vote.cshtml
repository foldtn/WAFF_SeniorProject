@model WAFF.DataAccess.ViewModels.Voting.EventVoteViewModel

@using System.Web.Optimization; 
@using Newtonsoft.Json
@{Layout = "~/Views/Shared/_reactLayout.cshtml"; ViewBag.Title = "Vote";} 

<div id="voting-mount">

</div>

@section scripts {
    @Scripts.Render("~/scripts/voting") 
    <script type="text/javascript" src="~/Scripts/jquery-1.10.2.min.js"></script>

    <script type="text/javascript">
        var VoteDetail = "" || []
        VoteDetail.Model = @Html.Raw(JsonConvert.SerializeObject(Model));
    </script>
    <script type="text/javascript">
        (function () {

            React.render(React.createElement(VotingContainer, {}), document.getElementById("voting-mount"));
         })();
    </script>
}
    